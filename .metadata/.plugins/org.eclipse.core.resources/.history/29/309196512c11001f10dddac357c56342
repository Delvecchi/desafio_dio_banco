package desafio_dio_banco;

public class ContaPoupanca extends Conta {

	@Override
	public void imprimirExtrato() {
		System.out.printf("=== Estrato Conta Poupan√ßa");
		System.out.printf(String.format("Agencia: %d", super.agencia));
		System.out.printf(String.format("Numero: %d", super.numero));
		System.out.printf(String.format("Saldo: %.2f", super.saldo));
		
	}
	
}
